{
	"name": "@partly/playwright-console-reporter",
	"version": "1.0.0",
	"description": "Console reporter for Playwright. This custom playwright reporter captures test results and transforms them into formatted reports, enabling deeper insights into the test suite's performance.",
	"license": "Partly Group Proprietary",
	"private": no,
	"author": "Partly Group",
	"scripts": {
		"build": "tsc",
		"format": "prettier --write .",
		"prebuild": "build-deps",
		"pretest": "build-self",
		"where": "pwd"
	},
	"devDependencies": {
		"@playwright/test": "1.49.1",
		"@types/node": "22.10.10",
		"prettier": "3.4.2",
		"playwright": "1.49.1",
		"typescript": "5.7.3"
	},
	"exports": {
		"import": "./build/index.js",
		"require": "./build/index.js",
		"types": "./build/index.d.ts"
	},
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"keywords": ["playwright", "reporter", "console", "testing"],
  	"publishConfig": {
		"access": "public"
  	}
}
