{
    "name": "playwright-console-reporter",
    "version": "1.0.2",
    "description": "Console reporter for Playwright. This custom playwright reporter captures test results and transforms them into formatted reports, enabling deeper insights into the test suite's performance.",
    "license": "MIT",
    "author": "Partly Group",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write .",
        "release": "npm publish --access public"
    },
    "dependencies": {
        "@playwright/test": "^1.57.0"
    },
    "devDependencies": {
        "@types/node": "22.10.10",
        "playwright": "^1.57.0",
        "prettier": "3.4.2",
        "typescript": "5.7.3"
    },
    "exports": {
        "import": "./build/index.js",
        "require": "./build/index.js",
        "types": "./build/index.d.ts"
    },
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "keywords": [
        "playwright",
        "reporter",
        "console",
        "testing"
    ],
    "publishConfig": {
        "access": "public"
    }
}
